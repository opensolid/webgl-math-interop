{
    "version": "1.0.0",
    "summary": "Interop support for opensolid/geometry and Zinggi/elm-webgl-math",
    "repository": "https://github.com/opensolid/webgl-math-interop.git",
    "license": "MPL-2.0",
    "source-directories": [
        "src"
    ],
    "exposed-modules": [
        "OpenSolid.Interop.WebGLMath.Frame2d",
        "OpenSolid.Interop.WebGLMath.Frame3d",
        "OpenSolid.Interop.WebGLMath.Point2d",
        "OpenSolid.Interop.WebGLMath.Point3d",
        "OpenSolid.Interop.WebGLMath.Vector2d",
        "OpenSolid.Interop.WebGLMath.Vector3d"
    ],
    "dependencies": {
        "Zinggi/elm-webgl-math": "1.0.5 <= v < 2.0.0",
        "elm-lang/core": "5.1.1 <= v < 6.0.0",
        "opensolid/geometry": "2.0.1 <= v < 3.0.0"
    },
    "elm-version": "0.18.0 <= v < 0.19.0"
}
